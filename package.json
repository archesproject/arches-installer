{
  "name": "arches-installer",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archesproject/arches-installer.git"
  },
  "bugs": {
    "url": "https://github.com/archesproject/arches-installer/issues"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron main.js --env=development",
    "package-osx": "electron-packager . ArchesInstaller --platform=darwin --arch=x64 --version=0.36.2 --icon assets/img/arches_logo.icns --out=dist",
    "package-windows": "electron-packager . ArchesInstaller --platform=win32 --arch=all --version=0.36.2 --icon assets/img/arches_logo.ico --out=dist",
    "package-linux": "electron-packager . ArchesInstaller --platform=linux --arch=all --version=0.36.2 --out=dist",
    "package-all": "npm run package-osx && npm run package-windows && npm run package-linux"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "bookshelf": "^0.9.1",
    "bootstrap": "^3.3.6",
    "bootstrap-select": "^1.9.3",
    "chosen": "0.0.2",
    "electron-sudo": "^3.0.2",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.5.0",
    "jquery": "^2.1.4",
    "knex": "^0.9.0",
    "knockout": "^3.4.0",
    "mustache": "^2.2.1",
    "pace": "0.0.4",
    "pg": "^4.4.3",
    "switchery": "0.0.2",
    "tree-kill": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.0"
  }
}
